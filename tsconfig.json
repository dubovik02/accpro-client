    "outDir": "./dist/",
    "module": "commonjs",
    "sourceMap": true,
    "noImplicitAny": true,
    "target": "es2015",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
  },
  "exclude": [
    "node_modules",
  ],
  "include": [
    "src/**/*.ts"
  ],
}